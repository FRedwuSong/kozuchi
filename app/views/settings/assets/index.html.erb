<%= render :partial => "new_asset_form",
   :locals => {:path => settings_assets_path, :account_type_name => Account::Asset.human_name} %>

<%= error_messages @account %>

<div class="bottom_box">
  <%= form_tag settings_assets_path, :method => :put do %>
  <table class="masters" width="100%">
    <tr>
      <th><%= Account::Asset.human_name %>名</th>
      <th>種別</th>
      <th>詳細</th>
      <th>並び順</th>
      <th>変更</th>
      <th>削除</th>
    </tr>
    <%= render :partial => "asset", :collection => @accounts  %>
  </table>
  <div class="operation"><%= link_to "編集を中止して再表示", settings_assets_path %></div>
  <% end  -%>

</div>